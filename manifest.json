{
  "name": "New Tab Manager",
  "version": "1.1",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com https://fonts.googleapis.com https://apis.google.com https://ajax.googleapis.com; object-src 'self'",
    "icons": { "128": "img/icon.png" },
  "default_locale" : "en",
  "offline_enabled": true,
  "description": "__MSG_extDesc__",
  "options_page": "options.html",
  "background": {
    "scripts": ["extData.js","newTabExt.js","background.js"],
    "persistent": false
  },
  "browser_action": {
      "default_title": "New Tab Manager",
      "default_icon": { "19": "img/icon19.png",
                       "38": "img/icon38.png"}
  },
  "permissions": [
    "management","tabs"
  ]
}